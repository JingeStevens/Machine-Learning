   "source": [
    "from numpy.linalg import inv\n",
    "\n",
    "#Finding the coefficients by normal equation\n",
    "if np.linalg.det(X.T.dot(X)) != 0:\n",
    "    W = (inv(X.T.dot(X)).dot(X.T)).dot(y)\n",
    "print(W)"
   ]
