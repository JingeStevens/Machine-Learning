   "source": [
    "import numpy as np\n",
    "\n",
    "x0 = np.ones((100, 1))\n",
    "\n",
    "#three variables\n",
    "x1 = np.random.rand(100, 1)\n",
    "x2 = np.random.rand(100, 1)\n",
    "x3 = np.random.rand(100, 1)\n",
    "\n",
    "#concat x0,x1,x2,x3\n",
    "X = np.concatenate((x0, x1, x2, x3), axis=1)\n",
    "print(X.shape)\n",
    "\n",
    "#true label\n",
    "y = 3*x1 + 4*x2 + 5*x3 + np.random.randn(100, 1) + 2  #randn()生成正太分布随机数"
   ]
  },
