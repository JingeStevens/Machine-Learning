    "# %matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "iters = 1000  #set the num of iteration\n",
    "W = np.random.randn(4,1)  #init the W\n",
    "\n",
    "loss_list = []  #to store all loss for each iter\n",
    "for iter in range(iters):\n",
    "    loss = .0\n",
    "    for i in range(X.shape[0]):\n",
    "        xi = X[i]\n",
    "        yi = y[i]\n",
    "        g = (xi.dot(W) - yi) * xi  #gradient\n",
    "        learning_rate = 4/(1 + iter + i) + 0.001  #learning_rate changes with iter and i\n",
    "        W -= learning_rate * g.reshape(4, 1)  #update the parameter W\n",
    "        loss += 0.5 * (xi.dot(W) - yi) ** 2  #sum loss for each sample\n",
    "        \n",
    "    print('loss in iter %d:%f'%(iter, loss))\n",
    "    loss_list.append(loss)\n",
    "    \n",
    "print(W)\n",
    "plt.plot(range(iters), loss_list)\n",
    "plt.xlabel('Iters')\n",
    "plt.ylabel('Loss')\n",
    "plt.show()\n",
    "plt.plot(range(2, iters), loss_list[2:])\n",
    "plt.xlabel('Iters')\n",
    "plt.ylabel('Loss')\n",
    "plt.show()\n",
    "    "
   ]
